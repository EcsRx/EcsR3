  <ItemGroup>
    <None Remove="Properties\**" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SystemsR3\SystemsR3.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Lookups\" />
  </ItemGroup>
